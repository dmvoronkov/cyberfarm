{
  "name": "cyberfarm",
  "version": "1.0.0",
  "description": "Game about surviving in post apocalyptic world",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "mig": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate",
    "seed": "npx sequelize db:seed:all",
    "createDB": "npx sequelize db:create",
    "dropDB": "npx sequelize db:drop",
    "dev": "nodemon app.js --ignore sessions --ext js,jsx",
    "start": "node app.js"
  },
  "keywords": [
    "game",
    "farm",
    "cyberpunk",
    "postapocalypsis"
  ],
  "author": "Dmitrii Voronkov",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.24.1",
    "@babel/preset-env": "^7.24.1",
    "@babel/preset-react": "^7.24.1",
    "@babel/register": "^7.23.7",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sequelize": "^6.37.1",
    "session-file-store": "^1.5.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.2"
  }
}
